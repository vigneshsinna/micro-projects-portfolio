{
	"compilerOptions": {
		"module": "commonjs",
		"target": "ES2020",
		"outDir": "out",
		"lib": [
			"ES2020"
		],
		"sourceMap": true,
		"rootDir": "src",
		"strict": true,
		"types": ["vscode", "node"],
		"skipLibCheck": true,
		"moduleResolution": "node"
	},
	"exclude": [
		"node_modules",
		".vscode-test"
	]
}